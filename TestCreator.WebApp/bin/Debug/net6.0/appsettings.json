{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=localhost;Initial Catalog=TestCreatorAngular;Integrated Security=True; MultipleActiveResultSets=True;"
  },
  "Auth": {
    "Jwt": {
      "Issuer": "http://www.testcreator.com/",
      "Audience": "http://www.testcreator.com/",
      "Key": "----unique_key----",
      "TokenExpirationInMinutes": 86400
    } 
  }, 
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "StaticFiles": {
    "Headers": {
      "Cache-Control": "max-age=3600",
      "Pragma": "cache",
      "Expires": "null"
    }
  }
}
